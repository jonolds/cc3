#!/bin/bash

javac src/main/java/WordCount.java src/main/java/NewText.java -Xlint:deprecation -d ./
jar -cvf WordCountReverse.jar ./*.class
rm ./*.class
hadoop jar WordCountReverse.jar WordCount input output
cat ./output/part-r-00000
cp src/main/java/*.java .
tar -cvf pa3_olds.tar input output ./*.java answer.txt
rm ./*.java


