#!/bin/bash

javac src/main/java/WordCount.java src/main/java/NewText.java -Xlint:deprecation -d ./target/classes
jar -cvf WordCountReverse.jar target/classes/*.class 
#rm -R output
hadoop jar WordCountReverse.jar WordCount input output
tar -cvf pa3_olds.tar input output src/main/java/WordCount.java NewText.java answer.txt
